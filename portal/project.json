{
    "name": "portal",
    "implicitDependencies": [
        "api"
    ],
    "targets": {
        "restore": {
            "executor": "nx:run-commands",
            "options": {
                "command": "npm install",
                "cwd": "{projectRoot}"
            }
        },
        "build": {
            "dependsOn": ["api:build"],
            "configurations": {
                "development": {},
                "production": {}
            },
            "defaultConfiguration": "production"
        }
    }
}